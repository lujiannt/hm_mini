/* pages/evaluate/evaluate.wxss */

.view-page {
  height: 100%;
  width: 100%;
  left: 0;
  top: 0;
  box-sizing: border-box;
  position: absolute;
}

.page-content {
  box-sizing: border-box;
  position: relative;
  margin-top: 130rpx;
}

/* 评论商品 */

.header_dis {
  display: flex;
  margin: 170rpx 25rpx 46rpx 25rpx;
}

.dis_img {
  width: 110rpx;
  height: 110rpx;
  margin-right: 20rpx;
}

.dis_name {
  font-size: 26rpx;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
/* 评价 */
.goods_eva{
  display: flex;
  align-items: center;
  margin: 0 0 40rpx 25rpx;
}
.eva_start{
  display: flex;
  align-items: center;
}
.eva_char{
  font-size: 32rpx;
  color: #333333;
  margin: 0 0 0 25rpx;
}
.start{
  width: 30rpx;
  height: 30rpx;
  margin: 0 0 0 20rpx;
}

