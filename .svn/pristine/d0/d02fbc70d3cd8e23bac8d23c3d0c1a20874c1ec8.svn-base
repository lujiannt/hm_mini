.navbar {
  width: 100%;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  background-color: white;
}

.in_navbar {
  width: 100%;
  overflow: hidden;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  background-image: url('https://jshm.oss-cn-shanghai.aliyuncs.com/other/20191231150133.png');
  background-size: 100% 100%;
  background-repeat: no-repeat;
}

